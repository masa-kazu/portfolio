<h1>食品編集</h1>



<%= form_with(model: @food, local: true) do |f| %>
<div class="field">
  <%= f.label :title, "食品名" %><br>
  <%= f.text_field :title %>
</div>
<div class="field">
  <%= f.label :content, "詳細" %><br>
  <%= f.text_area :content %>
</div>
<div class="field">
  <%= f.label :time, "賞味期限" %><br>
  <%= f.date_field :time %>
</div>

<div class="actions">
  <%= f.submit "編集完了", class: "btn btn-info" %>
</div>

<div class="actions">
  <%= link_to '戻る', foods_path, class: "btn btn-outline-secondary btn-sm" %>
</div>
<% end %>